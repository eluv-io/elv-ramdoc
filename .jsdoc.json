{
  "tags": {
    "allowUnknownTags": ["category","sig"],
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["publish.js"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "pedantic": true,
    "private": true,
    "recurse": true,
    "template": "."
  }
}
